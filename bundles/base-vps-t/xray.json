{
  "log": {
    "loglevel": "warning"
  },
  "inbounds": [
    {
      "tag": "vless-reality-in",
      "listen": "0.0.0.0",
      "port": 443,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "00000000-0000-4000-8000-000000000123",
            "email": "vps-e-transit"
          }
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "tcp",
        "security": "reality",
        "realitySettings": {
          "show": false,
          "dest": "splitter.wb.ru:443",
          "xver": 0,
          "serverNames": [
            "splitter.wb.ru"
          ],
          "privateKey": "REPLACE_PRIVATE_KEY",
          "shortIds": [
            "0123456789abcdef"
          ]
        }
      },
      "sniffing": {
        "enabled": true,
        "destOverride": [
          "http",
          "tls"
        ]
      }
    }
    ,
    {
      "tag": "vless-transit-in",
      "listen": "0.0.0.0",
      "port": 50000,
      "protocol": "vless",
      "settings": {
        "clients": [
          {
            "id": "00000000-0000-4000-8000-000000000123",
            "email": "vps-e-transit"
          }
        ],
        "decryption": "none"
      },
      "streamSettings": {
        "network": "tcp",
        "security": "none"
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "freedom",
      "tag": "direct"
    },
    {
      "protocol": "blackhole",
      "tag": "block"
    }
  ],
  "routing": {
    "rules": [
      {
        "type": "field",
        "ip": [
          "geoip:private"
        ],
        "outboundTag": "block"
      }
    ]
  }
}
